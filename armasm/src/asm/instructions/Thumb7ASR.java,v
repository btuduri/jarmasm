@Thumb support updates
@
text
@package asm.instructions;
/*
 * 12,11 Op; Opcode
              0 - LSL
              1 - LSR
              2 - ASR
 */
public class Thumb7ASR extends Thumb7Instruction {
    Thumb7Instruction i;
    
    public Thumb7ASR(ParsedInstruction pi) {
        super(pi);
        java.util.Vector opcodes = pi.getOpcodes();
        if (((String)opcodes.lastElement()).startsWith("#")) {
            i = new Thumb7Format1(pi);
            i.set(12);
        } else {
            i = new Thumb7Format4(pi);
            i.set(8);
        }
    }
    
    public int getMachineCode() {
        return i.getMachineCode();
    }
}
@


