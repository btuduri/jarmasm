@package asm.instructions;

public class Thumb7SUB extends Thumb7Instruction {
    Thumb7Instruction i;
    
    public Thumb7SUB(ParsedInstruction pi) {
        super(pi);
        java.util.Vector opcodes = pi.getOpcodes();
        if ( ((String)opcodes.elementAt(2)).startsWith("#") ) {
            i = new Thumb7Format3(pi);
            i.set(12);
            i.set(11);
        } else {        
            i = new Thumb7Format2(pi);
            i.set(9);
        }
    }
    
    public int getMachineCode() {
        return i.getMachineCode();
    }
}
@


